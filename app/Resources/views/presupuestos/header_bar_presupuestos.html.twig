            <div class="col-md-2">
                <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">Detalles Presupuesto</a>
            </div>
            {% if presupuesto %}
                <div class="datos_cliente_nombre col-md-6 col-md-offset-1"> {{ presupuesto.cliente.nombre|trim }}</div>
            {% else %}
                <div class="col-md-7 datos_cliente_nombre">&nbsp</div>
            {% endif %}
            <div class="col-md-2 col-md-offset-1">
                <div class="col-md-2"><span>TOTAL:</span></div>
                <div class="total_importe_top_bar col-md-6 col-md-offset-1 pull-right">&nbsp</div>
            </div>
        </div>
